{
  "output": {
    "filePath": "repomix-output.txt",
    "style": "xml",
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": false
  },
  "include": ["./afm-tailwind.css", "index.html", "input.css", "package.json", "tailwind.config.js"],
  "ignore": {
    "useGitignore": false,
    "useDefaultPatterns": false,
    "customPatterns": ["./node_modules/**", ".git"]
  },
  "security": {
    "enableSecurityCheck": true
  }
}
